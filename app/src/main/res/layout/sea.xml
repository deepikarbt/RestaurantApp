<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.deepika.restaurantapp.Sea">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true">
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:id="@+id/scrollView">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img"
                        android:src="@drawable/fideua"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Seafood fideua"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img1"
                        android:src="@drawable/korean"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt1"
                        android:layout_alignLeft="@id/img1"
                        android:layout_alignTop="@id/img1"
                        android:layout_alignRight="@id/img1"
                        android:layout_alignBottom="@id/img1"
                        android:text="Korean clam broth"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom" />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img2"
                        android:src="@drawable/bake"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt2"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="crab mac 'n' cheese bake"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img3"
                        android:src="@drawable/prawncock"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt3"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Classic prawn cocktail"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img4"
                        android:src="@drawable/mussel"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt4"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Mussel popcorn subs"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"

                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img5"
                        android:src="@drawable/prawncurry"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt5"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Prawn laksa curry bowl"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img6"
                        android:src="@drawable/stew"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt6"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Cod and chorizo stew"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img7"
                        android:src="@drawable/chowder"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt7"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Simple seafood chowder"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img8"
                        android:src="@drawable/prawnnood"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt8"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Super fast prawn noodles"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:padding="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentStart="true"
                        android:id="@+id/img9"
                        android:src="@drawable/tikka"
                        android:adjustViewBounds="true"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/txt9"
                        android:layout_alignLeft="@id/img"
                        android:layout_alignTop="@id/img"
                        android:layout_alignRight="@id/img"
                        android:layout_alignBottom="@id/img"
                        android:text="Tikka style fish"
                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#ffffff"
                        android:layout_gravity="center_horizontal|bottom"
                        />
                </FrameLayout>

            </LinearLayout>

        </ScrollView>
    </RelativeLayout>
</RelativeLayout>
