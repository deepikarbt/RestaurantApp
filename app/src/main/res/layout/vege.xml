                        android:gravity="center"
                        android:textStyle="italic|bold"
                        android:textColor="#000000"
                        android:layout_gravity="center_horizontal|bottom" />
                </FrameLayout>
                <FrameLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
